  "description": "Typescript package used to interact with the Etherscan (and other clones) api.",
  "main": "dist/index.js",
  "scripts": {
    "test": "yarn test",
    "build": "tsc",
    "watch": "tsc-watch --onSuccess \"node ./dist/demo.js\"",
